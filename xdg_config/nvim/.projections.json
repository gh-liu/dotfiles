{
  "after/lsp/*.lua": {
    "type": "lsp",
    "alternate": "lua/liu/lsp/servers/{}.lua"
  },
  "lua/liu/lsp/servers/*.lua": {
    "type": "lsp_ex",
    "alternate": "after/lsp/{}.lua",
    "template": [
      "---@type vim.lsp.Config",
      "local Config = {}",
      "return Config"
    ]
  },
  "lua/liu/lsp/init.lua": {
    "type": "lsp_init"
  },
  "lua/liu/dap/adapters/*.lua": {
    "type": "dap"
  },
  "lua/liu/dap/init.lua": {
    "type": "dap_init"
  },
  "lua/liu/config/*.lua": {
    "type": "config"
  },
  "lua/liu/plugins/*.lua": {
    "type": "plugin_spec"
  },
  "snippets/*.json": {
    "type": "snippet"
  },
  "after/ftplugin/*.lua": {
    "type": "ftplugin"
  },
  "after/plugin/*.lua": {
    "type": "after_plugin"
  },
  "after/plugin/*.vim": {
    "type": "after_plugin"
  }
}
